<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="visible_tab_count" format="integer" />

    <declare-styleable name="ViewPagerIndicator">
        <attr name="visible_tab_count" />
    </declare-styleable>

    <declare-styleable name="TabWidget">
        <attr name="bottom_labels" format="reference" />
    </declare-styleable>

    <declare-styleable name="RotateTextView">
        <attr name="degree" format="dimension" />
    </declare-styleable>
    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean" />
        <attr name="ios" format="boolean" />
        <attr name="leftSwipe" format="boolean" />
    </declare-styleable>
</resources>
